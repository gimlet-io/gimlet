{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "artifact list",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "cmd/cli",
            "args": [
                "artifact",
                "list"
            ],
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "release track",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "cmd/cli",
            "args": [
                "release",
                "track",
                "c3e857ce-0e4e-402a-8cbb-0325c06532be"
            ],
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "GimletD",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "cmd/gimletd",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "Gimlet Dashboard",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "cmd/dashboard",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "Installer",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "cmd/installer",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "bootstrap",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "cmd/cli",
            "args": [
                "gitops",
                "bootstrap",
                "--env",
                "live",
                "--gitops-repo-url",
                "git@github.com:szabogabor91/gitops-infra.git",
                "--gitops-repo-path",
                "/Users/szabogabor/gitops-infra"
            ],
            "envFile": "${workspaceFolder}/.env"
        },
    ]
}